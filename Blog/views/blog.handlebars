    <h1>Blog Posts</h1>

    <div id="blog-posts">
        {{#each blogPosts}}
            <div class="blog-post">
                <h2>{{title}}</h2>
                <p>{{contentPost}}</p>
                <p>Published on {{formatDate dateCreated}}</p>
            </div>
        {{/each}}
    </div>

